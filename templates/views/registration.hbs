    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <title>{{title}} - Registration</title>
  </head>
  <body>
      {{>header}}
      <form action="Registrace">
          <label for="name">Name:</label>
          <br>
          <input name="name" id="name">
